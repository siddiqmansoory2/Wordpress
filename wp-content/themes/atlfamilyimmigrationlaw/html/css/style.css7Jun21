section.cms-banner {
  position: relative;
  padding: 60px 0 30px;
  background-size: cover;
}

section.cms-banner .banner-top {
  display: none;
}

section.cms-banner .banner-inner {
  position: relative;
}

section.cms-banner .banner-inner:before {
  content: "";
  position: absolute;
  left: -20px;
  top: 3px;
  width: 5px;
  height: 175px;
  background-color: #F5C643;
}

section.cms-banner h1 {
  font-size: 14px;
  line-height: 19px;
  letter-spacing: 0.1em;
  text-transform: capitalize;
  color: #520D17;
  margin: 0 0 15px;
}

section.cms-banner h1 span {
  color: #C69716;
  display: block;
}

section.cms-banner h2 {
  font-size: 72px;
  line-height: 64px;
  letter-spacing: -0.02em;
  text-transform: uppercase;
  margin: 0 0 30px;
}

section.cms-banner p {
  font-family: 'Lato-Semibold';
  font-size: 16px;
  padding: 0 40px 30px;
  padding-right: 70px;
}

section.cms-banner .banner-bottom {
  display: flex;
}

section.cms-banner .btn-box .book-btn {
  position: relative;
  width: 340px;
  height: 70px;
  background-color: #FED35B;
  border: 5px solid #711B28;
  box-shadow: 0px 10px 14px rgba(0, 0, 0, 0.2);
  border-radius: 50px;
  font-family: 'Lato-ExtraBold';
  font-size: 21px;
  text-transform: uppercase;
  color: #711B28;
  display: flex;
  align-items: center;
  justify-content: center;
  mix-blend-mode: darken;
}

section.cms-banner .btn-box .book-btn img {
  margin-left: 20px;
  -webkit-transition: all 0.2s linear 0.2s;
  -o-transition: all 0.2s linear 0.2s;
  -moz-transition: all 0.2s linear 0.2s;
  transition: all 0.2s linear 0.2s;
}

section.cms-banner .btn-box .book-btn:hover img {
  -webkit-transform: translate(10px, 0);
  -moz-transform: translate(10px, 0);
  -ms-transform: translate(10px, 0);
  -o-transform: translate(10px, 0);
  transform: translate(10px, 0);
}

section.cms-banner .banner-bottom .g-review {
  display: flex;
  flex-wrap: wrap;
  font-family: 'Lato-Bold';
  font-size: 38px;
  color: #520D17;
  line-height: 1;
  padding-left: 24px;
}

section.cms-banner .banner-bottom .g-review img {
  margin-right: 5px;
}

section.cms-banner .banner-bottom .g-review span {
  font-family: 'Lato-Medium';
  font-size: 18px;
  display: block;
  width: 100%;
}

