html ,
body.home  {
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

body.home  {
  font-weight: normal;
  font-size: 18px !important;
  line-height: 30px !important;
  color: #424B55 !important;
  padding: 0;
  margin: 0;
  text-size-adjust: none;
  -webkit-text-size-adjust: none;
}

.home * {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -o-box-sizing: border-box;
  box-sizing: border-box;
}

 button {
  color: #cfa757;
  text-decoration: none;
  outline: none;
  display: inline-block;
  -webkit-transition: all 0.3s ease-in;
  -moz-transition: all 0.3s ease-in;
  -o-transition: all 0.3s ease-in;
  transition: all 0.3s ease-in;
}

 a {
  color: #536274;
  text-decoration: none;
  outline: none;
  display: inline-block;
  -webkit-transition: all 0.3s ease-in;
  -moz-transition: all 0.3s ease-in;
  -o-transition: all 0.3s ease-in;
  transition: all 0.3s ease-in;
}

 a:hover,
 a:focus {
  color: #FD9902;
  outline: none;
  text-decoration: none;
}

.home h1,
.home h2,
.home h3,
.home h4,
.home h5,
.home h6 {
  font-family: 'Open Sans', sans-serif !important;
  font-weight: 800 !important;
  color: #520D17 !important;
  margin-top: 0px !important;
}

.home p {
  margin-bottom: 30px;
}

.home p:last-child {
  margin-bottom: 0px;
}

.home ul,
.home ol {
  list-style: none;
  margin: 0;
  padding: 0;
}

.home img {
  max-width: 100%;
  height: auto;
}

.yellow-btn {
  font-family: 'Lato-ExtraBold';
  color: #F5C643;
  border: 4px solid #F5C643;
  border-radius: 50px;
  display: inline-block;
  font-size: 18px;
  padding: 8px 37px 8px 37px;
  position: relative;
  text-align: center;
  width: auto;
  cursor: pointer;
  background-color: transparent;
  text-transform: uppercase;
  line-height: 1.42857143;
}

.yellow-btn:hover,
.yellow-btn:focus {
  color: #520D17;
  border: solid 4px #F5C643;
  background-color: #F5C643;
}

.gray-btn {
  font-family: 'Lato-ExtraBold';
  color: #711B28;
  border: 4px solid #711B28;
  border-radius: 50px;
  display: inline-block;
  font-size: 18px;
  padding: 11px 37px 11px 37px;
  position: relative;
  text-align: center;
  width: auto;
  cursor: pointer;
  background-color: transparent;
  text-transform: uppercase;
  line-height: 1.42857143;
}

.gray-btn:hover,
.gray-btn:focus {
  color: #F5C643;
  border: solid 4px #711B28;
  background-color: #711B28;
}

.section-header {
  padding: 0 0 40px;
}

.section-header h2 {
